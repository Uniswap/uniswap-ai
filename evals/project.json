{
  "name": "evals",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "evals",
  "projectType": "library",
  "tags": ["type:evals"],
  "targets": {
    "eval": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx evals/framework/runner.ts",
        "forwardAllArgs": true
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx evals/framework/runner.ts",
        "forwardAllArgs": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsc --noEmit -p evals/tsconfig.json"
      }
    }
  }
}
