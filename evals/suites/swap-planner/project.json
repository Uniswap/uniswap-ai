{
  "name": "eval-suite-swap-planner",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "tags": ["type:eval-suite"],
  "implicitDependencies": ["uniswap-driver", "evals"],
  "targets": {
    "eval": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": [
        "{projectRoot}/**/*",
        "{workspaceRoot}/packages/plugins/uniswap-driver/skills/swap-planner/**/*",
        "{workspaceRoot}/evals/scripts/**/*",
        "{workspaceRoot}/evals/rubrics/**/*",
        "{workspaceRoot}/evals/promptfoo.yaml"
      ],
      "outputs": ["{projectRoot}/results.json"],
      "options": {
        "command": "npx promptfoo eval -c suites/swap-planner/promptfoo.yaml --output suites/swap-planner/results.json --no-progress-bar",
        "cwd": "evals"
      },
      "metadata": {
        "description": "Run swap-planner eval suite"
      }
    }
  }
}
