{
  "name": "uniswap-cca",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugins/uniswap-cca",
  "projectType": "library",
  "tags": ["type:plugin", "scope:uniswap", "domain:cca"],
  "targets": {
    "lint-markdown": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm exec markdownlint-cli2 -- 'packages/plugins/uniswap-cca/**/*.md'"
      }
    },
    "validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/validate-plugin.cjs packages/plugins/uniswap-cca"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python3 test_logic.py",
        "cwd": "packages/plugins/uniswap-cca/mcp-server/supply-schedule"
      }
    }
  }
}
